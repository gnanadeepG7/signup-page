<script type="text/javascript">
	$(function() {
	var paused0 = 0;
	$('#carouselExampleAutoplaying').carousel({
		interval: 4000,
		pause: "hover"
	  //pause: 0,
		// interval:5000,
	 //  pause: false
	});
  
	$('#toggleCarousels').click(function() {
	  var state0 = (paused0) ? 'cycle' : 'pause';
	  paused0 = (paused0) ? 0 : 1;
	  $('#carouselExampleAutoplaying').carousel(state0);
	  $(this).find('i').toggleClass('fa-play fa-pause');
	});
  });
  </script> 

