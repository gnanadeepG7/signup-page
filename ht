<script>

  $(function() {
  var paused = 0;
  $('#carouselExampleAutoplaying1').carousel({
    interval: 5000,
    pause: "hover"
    //pause: 0
  });

  $('#toggleCarousel').click(function() {
    var state = (paused) ? 'cycle' : 'pause';
    paused = (paused) ? 0 : 1;
    $('#carouselExampleAutoplaying1').carousel(state);
    $(this).find('i').toggleClass('fa-play fa-pause');
  });
});

</script>

