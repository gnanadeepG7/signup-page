      $(document).ready(function() {

        $('.showpersonal').click(function() {
          //console.log('working');
                $('.openpersonal').show();
                //$('.showaccount').removeClass("opening-menu");
        });

        $('.showgoldloan').click(function() {
                $('.opengoldloan').show();
        });

        $('.showinstaloan').click(function() {
                $('.openinstaloan').show();
        });

        $('.showcorporate').click(function() {
                $('.opencorporate').show();
        });

        $('.showagri').click(function() {
                $('.openagri').show();
        });

        $('.showmsme').click(function() {
                $('.openmsme').show();
        });

        $('.shownri').click(function() {
                $('.opennri').show();
        });

        $('.showacademic').click(function() {
                $('.openacademic').show();
        });

        $('.showtech').click(function() {
                $('.opentech').show();
        });

        $('.closebtn').click(function() {
          //console.log('working');
                $('.openpersonal').hide();
                $('.opengoldloan').hide();
                $('.openinstaloan').hide();
                $('.opencorporate').hide();
                $('.openagri').hide();
                $('.openmsme').hide();
                $('.opennri').hide();
                $('.openacademic').hide();
                $('.opentech').hide();
        });
        
    });

</script>

